@model List<KidKinder.Entities.AboutList>
<div class="col-lg-7 mb-5 mb-lg-0">
    @Html.Action("AboutDescriptionPartial", "Default")
    <ul class="list-inline m-0">
        @foreach (var item in Model)
        {
            <li class="py-2">
                <i class="fa fa-check text-success mr-3"></i>@item.Description
            </li>
        }
    </ul>
    <a href="/BookASeat/Index/" class="btn btn-primary mt-4 py-2 px-4">Book Now</a>
</div>
