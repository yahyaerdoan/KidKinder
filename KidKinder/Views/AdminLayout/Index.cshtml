@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">

@Html.Action("HeadPartial", "AdminLayout")

<body>
    <!-- Pre-loader start -->
    @Html.Action("PreLoaderPartial", "AdminLayout")
    <!-- Pre-loader end -->
    <div id="pcoded" class="pcoded">
        <div class="pcoded-overlay-box"></div>
        <div class="pcoded-container navbar-wrapper">
            @Html.Action("NavigatonBarPartial", "AdminLayout")

            <div class="pcoded-main-container">
                <div class="pcoded-wrapper">
                    @Html.Action("SideBarPartial", "AdminLayout")
                    <div class="pcoded-content">
                        @Html.Action("HeaderPartial", "AdminLayout")
                        @RenderBody()
                    </div>
                    <div id="styleSelector"></div>
                </div>
            </div>
        </div>
    </div>
